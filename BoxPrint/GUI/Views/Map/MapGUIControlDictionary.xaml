                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:BoxPrint.GUI.Views.Map">

    <Storyboard x:Key="storyboardSelectkiosk" AutoReverse="True" RepeatBehavior="Forever">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)" Storyboard.TargetName="pathSelectkiosk">
            <EasingDoubleKeyFrame KeyTime="0" Value="2.667"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.126" Value="-2.949"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="-10">
                <EasingDoubleKeyFrame.EasingFunction>
                    <ExponentialEase EasingMode="EaseOut"/>
                </EasingDoubleKeyFrame.EasingFunction>
            </EasingDoubleKeyFrame>
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="pathSelectkiosk">
            <EasingDoubleKeyFrame KeyTime="0" Value="0.852"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.126" Value="0.988"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="1"/>
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="pathSelectkiosk">
            <EasingDoubleKeyFrame KeyTime="0" Value="1.128"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.126" Value="1"/>
        </DoubleAnimationUsingKeyFrames>
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)" Storyboard.TargetName="pathSelectkiosk">
            <EasingDoubleKeyFrame KeyTime="0" Value="0.031"/>
            <EasingDoubleKeyFrame KeyTime="0:0:0.126" Value="0"/>
        </DoubleAnimationUsingKeyFrames>
    </Storyboard>

    <!--컨베어-->
    <Style x:Key="Conveyor" TargetType="{x:Type s:MapGUIControl}">
        <Setter Property="Height" Value="30"/>
        <Setter Property="Width"  Value="50"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type s:MapGUIControl}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <!--Background="#FFE2CCBE"-->
                        <Border CornerRadius="1" Background="{StaticResource SK_MouseOverGray}" Grid.ColumnSpan="6"/>

                        <!--Fill="#FF815360"-->
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1"/>
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1" Grid.Column="1"/>
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1" Grid.Column="2"/>
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1" Grid.Column="3"/>

                        <TextBlock Grid.Column="0" Grid.ColumnSpan="4" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                                   Foreground="#FF422030" FontFamily="{StaticResource ExtraBoldFont}"
                                   Text="{Binding Path=(s:MapGUIControl.DisplayName), RelativeSource={RelativeSource TemplatedParent}}"
                                   FontSize="{Binding Path=(s:MapGUIControl.DisplayNameFontSize), RelativeSource={RelativeSource TemplatedParent}}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--컨베어2-->
    <Style x:Key="Conveyor2" TargetType="{x:Type s:MapGUIControl}">
        <Setter Property="Height" Value="30"/>
        <Setter Property="Width"  Value="50"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type s:MapGUIControl}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <!--Background="#FFE2CCBE"-->
                        <Border CornerRadius="1" Background="{StaticResource SK_MouseOverGray}" Grid.ColumnSpan="6"/>

                        <!--Fill="#FF815360"-->
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1"/>
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1" Grid.Column="1"/>
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1" Grid.Column="2"/>
                        <Rectangle Fill="{StaticResource SK_LBlack}" RadiusX="1" RadiusY="1" Margin="1" Grid.Column="3"/>

                        <TextBlock Grid.Column="0" Grid.ColumnSpan="4" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                                   Foreground="#FF422030" FontFamily="{StaticResource ExtraBoldFont}"
                                   Text="{Binding Path=(s:MapGUIControl.DisplayName), RelativeSource={RelativeSource TemplatedParent}}"
                                   FontSize="{Binding Path=(s:MapGUIControl.DisplayNameFontSize), RelativeSource={RelativeSource TemplatedParent}}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!--랙마스터-->
    <Style x:Key="RackMaster" TargetType="{x:Type s:MapGUIControl}">
        <Setter Property="Height" Value="300"/>
        <Setter Property="Width"  Value="28"/>
        <Setter Property="Cursor"  Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type s:MapGUIControl}">
                    <Border Name="border" CornerRadius="3,5,0,5" BorderBrush="{Binding Path=(s:MapGUIControl.SubColor), RelativeSource={RelativeSource TemplatedParent}}" BorderThickness="0,0,4,10" >
                        <!--BorderBrush="#FF49596D"-->
                        <Border.Style>
                            <Style TargetType="Border">
                                <Setter Property="Background" Value="{Binding Path=(s:MapGUIControl.MainColor), RelativeSource={RelativeSource TemplatedParent}}"/>
                                <!--Value="#FFFBDB4C"-->
                                <!--<Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Background" Value="{Binding Path=(s:MapGUIControl.MouseOverColor), RelativeSource={RelativeSource TemplatedParent}}"/> -->
                                <!--Value="#FFFBF05A"-->
                                    <!--</Trigger>
                                </Style.Triggers>-->
                            </Style>
                        </Border.Style>

                        <!--Foreground="#FF422030"-->
                        <TextBlock Name="textBlock" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                                   Foreground="{TemplateBinding Foreground}" FontFamily="{StaticResource ExtraBoldFont}"
                                   Text="{Binding Path=(s:MapGUIControl.DisplayName), RelativeSource={RelativeSource TemplatedParent}}"
                                   FontSize="{Binding Path=(s:MapGUIControl.DisplayNameFontSize), RelativeSource={RelativeSource TemplatedParent}}" RenderTransformOrigin="0.5,0.5" >
                            <TextBlock.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="1" ScaleY="1"/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-90"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </TextBlock.LayoutTransform>
                        </TextBlock>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <!--<Setter TargetName="border"     Property="Background" Value="{Binding Path=(s:MapGUIControl.MouseOverColor), RelativeSource={RelativeSource TemplatedParent}}"/>
                            <Setter TargetName="textBlock"  Property="Foreground" Value="white"/>-->
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard >
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" From="1" To="1.3" Duration="0:0:0.2" />
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" From="1" To="1.3" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard >
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"  To="1" Duration="0:0:0.2" />
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"  To="1" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=(s:MapGUIControl.isSelect), RelativeSource={RelativeSource self}}" Value="True">
                            <Setter Property="Background" TargetName="border" Value="{StaticResource SK_Orange}"></Setter>
                            <Setter Property="Foreground" TargetName="textBlock" Value="white"></Setter>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!--랙마스터_번역-->
    <Style x:Key="RackMaster1" TargetType="{x:Type s:MapGUIControl}">
        <Setter Property="Height" Value="300"/>
        <Setter Property="Width"  Value="28"/>
        <Setter Property="Cursor"  Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type s:MapGUIControl}">
                    <Border Name="border" CornerRadius="3,5,0,5" BorderBrush="{Binding Path=(s:MapGUIControl.SubColor), RelativeSource={RelativeSource TemplatedParent}}" BorderThickness="0,0,4,10" >
                        <!--BorderBrush="#FF49596D"-->
                        <Border.Style>
                            <Style TargetType="Border">
                                <Setter Property="Background" Value="{Binding Path=(s:MapGUIControl.MainColor), RelativeSource={RelativeSource TemplatedParent}}"/>
                            </Style>
                        </Border.Style>

                        <!--Foreground="#FF422030"-->
                        <TextBlock Name="textBlock" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center"
                                   Foreground="{TemplateBinding Foreground}" FontFamily="{StaticResource ExtraBoldFont}"
                                   Text="{TemplateBinding Content}" FontSize="{TemplateBinding FontSize}" RenderTransformOrigin="0.5,0.5" >
                            <TextBlock.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="1" ScaleY="1"/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-90"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </TextBlock.LayoutTransform>
                        </TextBlock>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard >
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" From="1" To="1.3" Duration="0:0:0.2" />
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" From="1" To="1.3" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard >
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"  To="1" Duration="0:0:0.2" />
                                        <DoubleAnimation Storyboard.TargetName="textBlock" Storyboard.TargetProperty="(LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"  To="1" Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=(s:MapGUIControl.isSelect), RelativeSource={RelativeSource self}}" Value="True">
                            <Setter Property="Background" TargetName="border" Value="{StaticResource SK_Orange}"></Setter>
                            <Setter Property="Foreground" TargetName="textBlock" Value="white"></Setter>
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

