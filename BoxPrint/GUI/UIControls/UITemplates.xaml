    <!--//220610 HHJ SCS 개선     //- Crane UIControl 변경-->
    <Style x:Key="SK_RMStyleNew" TargetType="{x:Type u:UIControlRM}">
        <Setter Property="Width" Value="150"/>
        <Setter Property="Height" Value="50"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type u:UIControlRM}">
                    <ControlTemplate.Resources>
                        <Storyboard x:Key="sbLStateError">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.StrokeThickness)" Storyboard.TargetName="LState">
                                <EasingDoubleKeyFrame   KeyTime="0" Value="0"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Key="sbLStateNoneError">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.StrokeThickness)" Storyboard.TargetName="LState">
                                <EasingDoubleKeyFrame   KeyTime="0" Value="0.5"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Key="sbRStateError">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.StrokeThickness)" Storyboard.TargetName="RState">
                                <EasingDoubleKeyFrame   KeyTime="0" Value="0"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                        <Storyboard x:Key="sbRStateNoneError">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.StrokeThickness)" Storyboard.TargetName="RState">
                                <EasingDoubleKeyFrame   KeyTime="0" Value="0.5"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </ControlTemplate.Resources>
                    
                    <Grid x:Name="RmMain">
                        <!--//221129 YSW Layout RM UnitName 표시-->
                        <Grid x:Name="TextParts" Margin="0,-8,0,-8" IsHitTestVisible="False" Panel.ZIndex="9999">
                            <TextBlock FontSize="8" TextWrapping="NoWrap" Text="{Binding Path=ControlName, RelativeSource={RelativeSource TemplatedParent}}"
                                       TextAlignment="Center"/>
                        </Grid>
                        <!--//241001 HDK Crane 작업가능상태 표시 개선-->
                        <Grid x:Name="SCModeParts" IsHitTestVisible="False" Panel.ZIndex="9999" Opacity="0.8">
                            <Path Data="M29.999998,0 L85,55 140,0 C162.42341,-2.4234113 173.78222,6.2177771 170,29.999998 L115,85 170,140 C173.40814,163.40818 162.96851,172.96855 140,170 L85,115 29.999998,170 C6.3335905,173.66648 -3.6663026,163.66637 0,140 L55,85 0,29.999998 C-3.1306463,6.8693423 6.7962374,-3.2037727 29.999998,0 z" 
                                  Fill="Red" Margin="5" Stretch="Fill"/>
                            <Grid.Style>
                                <Style TargetType="{x:Type Grid}">
                                    <Setter Property="Visibility" Value="Visible"/>
                                </Style>
                            </Grid.Style>
                        </Grid>
                        
                        <Border CornerRadius="1" BorderBrush="Black" BorderThickness="1"
                                Background="White" ClipToBounds="False">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <Path Style="{StaticResource SCState}"
                                      x:Name="LState" Grid.Column="0" StrokeThickness="0.5"
                                      Fill="{Binding Path=CraneState, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>

                                <Path Style="{StaticResource SCState}"
                                      x:Name="RState" Grid.Column="2" StrokeThickness="0.5"
                                      Fill="{Binding Path=CraneState, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>

                                <Border Grid.Column="1"
                                        Background="{Binding Path=CraneSC_State, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                    <Border x:Name="arm" Grid.Column="1" Panel.ZIndex="500">
                                        <Border.RenderTransform>
                                            <TransformGroup>
                                                <!--//220919 HHJ SCS 개선     //- ForkAxisPosition Biding Item 변경-->
                                                <!--<TranslateTransform Y="{Binding Path=UIForkAxisPosition, RelativeSource={RelativeSource Mode=TemplatedParent}}"/>-->
                                                <TranslateTransform Y="{Binding Path=ForkAxisPosition, Converter={StaticResource YPositionConverter}, RelativeSource={RelativeSource Mode=TemplatedParent}}"/>
                                            </TransformGroup>
                                        </Border.RenderTransform>

                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="0.5*"/>
                                                <ColumnDefinition Width="0.5*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="3*"/>
                                                <ColumnDefinition Width="0.5*"/>
                                                <ColumnDefinition Width="0.5*"/>
                                            </Grid.ColumnDefinitions>

                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="0.5*"/>
                                                <RowDefinition Height="0.5*"/>
                                                <RowDefinition Height="8*"/>
                                                <RowDefinition Height="0.5*"/>
                                                <RowDefinition Height="0.5*"/>
                                            </Grid.RowDefinitions>

                                            <Border Grid.Column="2" Grid.Row="2" CornerRadius="1"
                                                    BorderThickness="0" Background="#FFB3BBC4"/>
                                            <Border Grid.Column="4" Grid.Row="2" CornerRadius="1"
                                                    BorderThickness="0" Background="#FFB3BBC4"/>

                                            <Border x:Name="LoadMaterial"
                                                    Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="5" Grid.RowSpan="3"
                                                    VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Opacity="0" IsHitTestVisible="False" CornerRadius="1"
                                                    BorderBrush="{Binding Path=SlotList[0].MaterialType, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                                <!--230911 HHJ UNK Mark 회전 추가 RenderTransformOrigin="0.5,0.5" 추가--> 
                                                <Path x:Name="UnkMark" Visibility="Collapsed" Fill="Red" StrokeThickness="0" Stretch="Uniform" RenderTransformOrigin="0.5,0.5"
                                                      Data="M80 160c0-35.3 28.7-64 64-64h32c35.3 0 64 28.7 64 64v3.6c0 21.8-11.1 42.1-29.4 53.8l-42.2 27.1c-25.2 16.2-40.4 44.1-40.4 74V320c0 17.7 14.3 32 32 32s32-14.3 32-32v-1.4c0-8.2 4.2-15.8 11-20.2l42.2-27.1c36.6-23.6 58.8-64.1 58.8-107.7V160c0-70.7-57.3-128-128-128H144C73.3 32 16 89.3 16 160c0 17.7 14.3 32 32 32s32-14.3 32-32zm80 320a40 40 0 1 0 0-80 40 40 0 1 0 0 80z"/>
                                            </Border>
                                        </Grid>
                                    </Border>
                                </Border>
                                
                            </Grid>
                        </Border>

                        <!--Selector-->
                        <Rectangle Stroke="{StaticResource SK_Orange}" StrokeThickness="3" StrokeDashArray="1"  Margin="0" Panel.ZIndex="9999"
                                   Visibility="{Binding Path=Selector, Converter={StaticResource PvisibleConverter}, RelativeSource={RelativeSource TemplatedParent}}" />
                    </Grid>

                    <ControlTemplate.Triggers>
                        <DataTrigger Binding="{Binding Path=CraneState, RelativeSource={RelativeSource Self}}" Value="ERROR">
                            <DataTrigger.ExitActions>
                                <BeginStoryboard Storyboard="{StaticResource sbLStateNoneError}"/>
                                <BeginStoryboard Storyboard="{StaticResource sbRStateNoneError}"/>
                            </DataTrigger.ExitActions>
                            <DataTrigger.EnterActions>
                                <BeginStoryboard Storyboard="{StaticResource sbLStateError}"/>
                                <BeginStoryboard Storyboard="{StaticResource sbRStateError}"/>
                            </DataTrigger.EnterActions>
                        </DataTrigger>

                        <!--//241001 HDK Crane 작업가능상태 표시 개선-->
                        <DataTrigger Binding="{Binding Path=CraneSCStatus,RelativeSource={RelativeSource Self}}" Value="AUTO_RUN">
                            <Setter TargetName="SCModeParts" Property="Visibility" Value="Collapsed"/>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding Path=SlotList[0].MaterialExist, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="LoadMaterial" Property="Opacity" Value="1"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=SlotList[0].MaterialExist, RelativeSource={RelativeSource Self}}" Value="False">
                            <Setter TargetName="LoadMaterial" Property="Opacity" Value="0"/>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle0">
                            <Setter TargetName="RmMain" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="TextParts" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="RmMain" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <Setter TargetName="TextParts" Property="RenderTransform" Value="{StaticResource Angle0}"/>

                            <!--230911 HHJ UNK Mark 회전 추가 Start-->
                            <Setter TargetName="UnkMark" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <!--230911 HHJ UNK Mark 회전 추가 End-->
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle90">
                            <Setter TargetName="RmMain" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="TextParts" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="RmMain" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <Setter TargetName="TextParts" Property="RenderTransform" Value="{StaticResource Angle0}"/>

                            <!--230911 HHJ UNK Mark 회전 추가 Start-->
                            <Setter TargetName="UnkMark" Property="RenderTransform" Value="{StaticResource AngleMinus90}"/>
                            <!--230911 HHJ UNK Mark 회전 추가 End-->
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle180">
                            <Setter TargetName="RmMain" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="TextParts" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="RmMain" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <Setter TargetName="TextParts" Property="RenderTransform" Value="{StaticResource Angle180}"/>

                            <!--230911 HHJ UNK Mark 회전 추가 Start-->
                            <Setter TargetName="UnkMark" Property="RenderTransform" Value="{StaticResource Angle180}"/>
                            <!--230911 HHJ UNK Mark 회전 추가 End-->
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle270">
                            <Setter TargetName="RmMain" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="TextParts" Property="RenderTransformOrigin" Value="0.5,0.5"/>
                            <Setter TargetName="RmMain" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <Setter TargetName="TextParts" Property="RenderTransform" Value="{StaticResource Angle0}"/>

                            <!--230911 HHJ UNK Mark 회전 추가 Start-->
                            <Setter TargetName="UnkMark" Property="RenderTransform" Value="{StaticResource AngleMinus270}"/>
                            <!--230911 HHJ UNK Mark 회전 추가 End-->
                        </DataTrigger>

                        <DataTrigger Binding="{Binding Path=SlotList[0].MaterialName, Converter={StaticResource idUnkConverter}, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="LoadMaterial" Property="Background" Value="Gold"/>
                            <Setter TargetName="UnkMark" Property="Visibility" Value="Visible"/>
                        </DataTrigger>
