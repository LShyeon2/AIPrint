                    <Viewbox>
                        <Grid Background="Transparent" Margin="1">
                            <Border x:Name="ShelfMainParts" Width="48" Height="48" >
                                <!--Background="{Binding Path=ShelfStatus, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}">-->
                                <!--//231101 HHJ Shelf NG State 추가-->
                                <Path x:Name="ShelfNGParts" Style="{StaticResource NGPath}" Visibility="Collapsed"/>

                                <!--//240719 HoN ProductEmpty 구분 추가-->
                                <Border.Resources>
                                    <Style TargetType="{x:Type Border}">
                                        <Setter Property="Background" Value="{Binding Path=ShelfStatus, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
                                        <Setter Property="BorderBrush" Value="{Binding Path=ShelfType, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
                                        <Setter Property="BorderThickness" Value="1"/>
                                        <Style.Triggers>
                                            <MultiDataTrigger>
                                                <MultiDataTrigger.Conditions>
                                                    <!--화물이 감지될때만 진행함.-->
                                                    <Condition Binding="{Binding Path=SlotList[0].MaterialExist, RelativeSource={RelativeSource TemplatedParent}}" Value="True"/>
                                                    <Condition Binding="{Binding Path=SlotList[0].ProductEmpty, RelativeSource={RelativeSource TemplatedParent}}" Value="EMPTY"/>
                                                </MultiDataTrigger.Conditions>
                                                <!--//2024.07.24 lim, 흰색 바탕에 테두리로 변경-->
                                                <Setter Property="Background" Value="White"/>
                                                <Setter Property="BorderBrush" Value="{Binding Path=ShelfStatus, Converter={StaticResource brushConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
                                                <Setter Property="BorderThickness" Value="7"/>
                                            </MultiDataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Border.Resources>
                            </Border>

                            <Border x:Name="DeadZoneShelfParts" BorderThickness="1" Width="48" Height="48" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5"
                                    BorderBrush="#000000"
                                    Background="#808080">
                                <Viewbox VerticalAlignment="Bottom" Margin="2">
                                    <TextBlock Text="Dead Cell"/>
                                </Viewbox>
                            </Border>
                            
                            <!--//220513 YSW SCS 개선 //- 연기감지 템플릿 추가-->
                            <!--221006 화재감지 테스트-->
                            <!--//SuHwan_20230317 : 화재감지 이미지 변경-->
                            <Grid x:Name="gridFire" Width="48" Height="48" RenderTransformOrigin="0.5,0.5"
                                  Visibility="{Binding Path=FireSensorValue, Converter={StaticResource PvisibleConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                <Path x:Name="pathFire" Style="{StaticResource FireImage}" Margin="0" Width="{TemplateBinding Width, Converter={StaticResource FireSizeConverter}}" Height="{TemplateBinding Height}"/>
                            </Grid>

                            <Grid x:Name="MemoParts" IsHitTestVisible="False" RenderTransformOrigin="0.5,0.5" Background="Transparent" Width="48" Height="48"
                                  Visibility="{Binding Path=ShelfMemo, Converter={StaticResource MemoVisibleConverter}, RelativeSource={RelativeSource TemplatedParent}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="5"/>
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="5"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="5"/>
                                </Grid.RowDefinitions>
                                <Path Grid.Column="3" Grid.Row="1" Data="M0,0 L100,0 L100,100 L0,100 z"
                                      Stretch="Fill" Fill="Gold"
                                      Stroke="Black" StrokeThickness="2"/>
                            </Grid>
                            
                            <!--//230118 HHJ SCS 개선-->
                            <Rectangle Stroke="{StaticResource SK_Orange}" StrokeThickness="3" StrokeDashArray="1"  Margin="0" Panel.ZIndex="9999"
                                       Width="48" Height="48"
                                       Visibility="{Binding Path=Selector, Converter={StaticResource PvisibleConverter}, RelativeSource={RelativeSource TemplatedParent}}"/>
                        </Grid>
                    </Viewbox>
                    <ControlTemplate.Triggers>
                        <!--//220513 YSW SCS 개선 //- 연기감지 템플릿 추가--><!--221006 화재감지 테스트-->
                        <DataTrigger Binding="{Binding Path=FireSensorValue, RelativeSource={RelativeSource Self}}" Value="True">
                            <DataTrigger.ExitActions>
                                <StopStoryboard BeginStoryboardName="Storyboard2"/>
                            </DataTrigger.ExitActions>
                            <DataTrigger.EnterActions>
                                <BeginStoryboard x:Name="Storyboard2" Storyboard="{StaticResource Storyboard2}"/>
                            </DataTrigger.EnterActions>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle0">
                            <Setter TargetName="MemoParts" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <Setter TargetName="gridFire" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                            <Setter TargetName="DeadZoneShelfParts" Property="RenderTransform" Value="{StaticResource Angle0}"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle90">
                            <Setter TargetName="MemoParts" Property="RenderTransform" Value="{StaticResource AngleMinus90}"/>
                            <Setter TargetName="gridFire" Property="RenderTransform" Value="{StaticResource AngleMinus90}"/>
                            <Setter TargetName="DeadZoneShelfParts" Property="RenderTransform" Value="{StaticResource AngleMinus90}"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle180">
                            <Setter TargetName="MemoParts" Property="RenderTransform" Value="{StaticResource AngleMinus180}"/>
                            <Setter TargetName="gridFire" Property="RenderTransform" Value="{StaticResource AngleMinus180}"/>
                            <Setter TargetName="DeadZoneShelfParts" Property="RenderTransform" Value="{StaticResource AngleMinus180}"/>
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=LayOutAngle, RelativeSource={RelativeSource Self}}" Value="eAngle270">
                            <Setter TargetName="MemoParts" Property="RenderTransform" Value="{StaticResource AngleMinus270}"/>
                            <Setter TargetName="gridFire" Property="RenderTransform" Value="{StaticResource AngleMinus270}"/>
                            <Setter TargetName="DeadZoneShelfParts" Property="RenderTransform" Value="{StaticResource AngleMinus270}"/>
                        </DataTrigger>

                        <DataTrigger Binding="{Binding Path=DeadZone, RelativeSource={RelativeSource Self}}" Value="True">
